{
  "name": "@trimble-oss/modus-web-components",
  "version": "0.0.16",
  "description": "Trimble Modus Web Component Library",
  "homepage": "https://github.com/trimble-oss/modus-web-components",
  "bugs": {
    "url": "https://github.com/trimble-oss/modus-web-components/issues/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trimble-oss/modus-web-components"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/trimble-oss"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/modus-web-components/modus-web-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --silent",
    "test.watch": "stencil test --spec --e2e --silent --watchAll",
    "generate": "stencil generate",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-js": "eslint src/** --no-error-on-unmatched-pattern",
    "lint-css": "stylelint src/**/*.scss",
    "full": "npm ci && npm run lint-js && npm run lint-css && npm run build && npm run test"
  },
  "dependencies": {
    "@stencil/core": "2.9.0"
  },
  "devDependencies": {
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^27.0.2",
    "@types/puppeteer": "^5.4.4",
    "@types/puppeteer-core": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "jest-cli": "26.5.3",
    "puppeteer": "^11.0.0",
    "puppeteer-core": "^11.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "typescript": "^4.4.4"
  },
  "license": "UNLICENSED"
}
