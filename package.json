{
  "name": "@trimble-oss/modus-web-components",
  "version": "0.0.19",
  "description": "Trimble Modus Web Component Library",
  "homepage": "https://github.com/trimble-oss/modus-web-components",
  "bugs": {
    "url": "https://github.com/trimble-oss/modus-web-components/issues/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trimble-oss/modus-web-components"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/trimble-oss"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/modus-web-components/modus-web-components.esm.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --silent",
    "test.watch": "stencil test --spec --e2e --silent --watchAll",
    "generate": "stencil generate",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-js": "eslint src/** --no-error-on-unmatched-pattern",
    "lint-css": "stylelint src/**/*.scss",
    "full": "npm ci && npm run lint-js && npm run lint-css && npm run build && npm run test && npm run build-storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./storybook/dist"
  },
  "dependencies": {
    "@stencil/core": "2.9.0",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addons": "^6.4.9",
    "@storybook/components": "^6.4.9",
    "@storybook/theming": "^6.4.9",
    "@types/mdx": "^2.0.1",
    "lit-html": "^2.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/web-components": "^6.4.9",
    "@types/jest": "^27.0.3",
    "@types/puppeteer": "^5.4.4",
    "@types/puppeteer-core": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.4.0",
    "eslint-plugin-storybook": "^0.5.3",
    "jest": "^27.4.3",
    "jest-cli": "26.5.3",
    "puppeteer": "^12.0.1",
    "puppeteer-core": "^12.0.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.2"
  },
  "license": "UNLICENSED"
}
